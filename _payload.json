:8,"title":520,"description":922,"date":923,"customer":924,"workLink":921,"websiteLink":925,"videoLink":926,"technologies":927,"body":929,"_type":493,"_id":1157,"_source":495,"_file":1158,"_stem":1159,"_extension":498},"/works/weather-dashboard","Piccola esercitazione di stile, integrare un api del meteo e visualizzarlo con un design moderno ed accattivante..","01/02/2024","Personal Project","https://weather-dashboard-lilac.vercel.app/","/video/works/weather-dashboard.mp4",[507,928,21],"Tailwind",{"type":23,"children":930,"toc":1153},[931,935,944],{"type":26,"tag":515,"props":932,"children":933},{"id":517},[934],{"type":32,"value":520},{"type":26,"tag":936,"props":937,"children":938},"the-problem",{},[939],{"type":26,"tag":35,"props":940,"children":941},{},[942],{"type":32,"value":943},"Piccola esercitazione di stile, integrare un api del meteo e\nvisualizzarlo con un design moderno ed accattivante..",{"type":26,"tag":945,"props":946,"children":947},"video-container",{"src":926},[948,957,963,973,981,1060,1066,1071,1079,1122,1130],{"type":26,"tag":35,"props":949,"children":950},{},[951],{"type":26,"tag":952,"props":953,"children":956},"img",{"alt":954,"src":955},"clear weather","/img/works/weather-dashboard/clear-weather.png",[],{"type":26,"tag":27,"props":958,"children":960},{"id":959},"esercitazione",[961],{"type":32,"value":962},"Esercitazione",{"type":26,"tag":35,"props":964,"children":965},{},[966,971],{"type":26,"tag":479,"props":967,"children":968},{},[969],{"type":32,"value":970},"Obiettivo:",{"type":32,"value":972}," Creare un'applicazione web che visualizzi le informazioni meteo per diverse città. L'applicazione deve essere reattiva, intuitiva e visivamente accattivante.",{"type":26,"tag":35,"props":974,"children":975},{},[976],{"type":26,"tag":479,"props":977,"children":978},{},[979],{"type":32,"value":980},"Requisiti:",{"type":26,"tag":104,"props":982,"children":983},{},[984,994],{"type":26,"tag":108,"props":985,"children":986},{},[987,992],{"type":26,"tag":479,"props":988,"children":989},{},[990],{"type":32,"value":991},"Selezione Città:",{"type":32,"value":993}," L'utente deve poter selezionare una città da un elenco predefinito.",{"type":26,"tag":108,"props":995,"children":996},{},[997,1002,1004],{"type":26,"tag":479,"props":998,"children":999},{},[1000],{"type":32,"value":1001},"Visualizzazione Dati Meteo:",{"type":32,"value":1003}," Visualizzare i dati meteo attuali per la città selezionata, inclusi:\n",{"type":26,"tag":104,"props":1005,"children":1006},{},[1007,1012,1017,1022,1027,1032,1037,1047,1057],{"type":26,"tag":108,"props":1008,"children":1009},{},[1010],{"type":32,"value":1011},"Temperatura",{"type":26,"tag":108,"props":1013,"children":1014},{},[1015],{"type":32,"value":1016},"Descrizione del tempo (es. soleggiato, nuvoloso, pioggia)",{"type":26,"tag":108,"props":1018,"children":1019},{},[1020],{"type":32,"value":1021},"Icona meteo",{"type":26,"tag":108,"props":1023,"children":1024},{},[1025],{"type":32,"value":1026},"Umidità",{"type":26,"tag":108,"props":1028,"children":1029},{},[1030],{"type":32,"value":1031},"Velocità del vento",{"type":26,"tag":108,"props":1033,"children":1034},{},[1035],{"type":32,"value":1036},"Precipitazioni",{"type":26,"tag":108,"props":1038,"children":1039},{},[1040,1045],{"type":26,"tag":479,"props":1041,"children":1042},{},[1043],{"type":32,"value":1044},"Dettagli Aggiuntivi:",{"type":32,"value":1046}," Includere una sezione espandibile per visualizzare informazioni meteo aggiuntive.",{"type":26,"tag":108,"props":1048,"children":1049},{},[1050,1055],{"type":26,"tag":479,"props":1051,"children":1052},{},[1053],{"type":32,"value":1054},"Design:",{"type":32,"value":1056}," L'interfaccia utente deve essere moderna e gradevole.",{"type":26,"tag":108,"props":1058,"children":1059},{},[],{"type":26,"tag":27,"props":1061,"children":1063},{"id":1062},"soluzione-personale",[1064],{"type":32,"value":1065},"Soluzione Personale:",{"type":26,"tag":35,"props":1067,"children":1068},{},[1069],{"type":32,"value":1070},"Per soddisfare i requisiti di animazione fluida e reattività, ho scelto di utilizzare la libreria **GSAP (GreenSock Animation Platform)**. GSAP offre un controllo preciso sulle animazioni e consente di creare effetti di transizione complessi con facilità.",{"type":26,"tag":35,"props":1072,"children":1073},{},[1074],{"type":26,"tag":479,"props":1075,"children":1076},{},[1077],{"type":32,"value":1078},"Esempi di animazioni implementate con GSAP:",{"type":26,"tag":104,"props":1080,"children":1081},{},[1082,1092,1102,1112],{"type":26,"tag":108,"props":1083,"children":1084},{},[1085,1090],{"type":26,"tag":479,"props":1086,"children":1087},{},[1088],{"type":32,"value":1089},"Transizioni fluide:",{"type":32,"value":1091}," Al cambio di città, i dati meteo vengono aggiornati con una transizione graduale, evitando cambiamenti bruschi nell'interfaccia.",{"type":26,"tag":108,"props":1093,"children":1094},{},[1095,1100],{"type":26,"tag":479,"props":1096,"children":1097},{},[1098],{"type":32,"value":1099},"Effetto di \"entrata\":",{"type":32,"value":1101}," Quando i dati meteo vengono caricati, gli elementi dell'interfaccia appaiono con un effetto di animazione (es. fade-in, slide-up).",{"type":26,"tag":108,"props":1103,"children":1104},{},[1105,1110],{"type":26,"tag":479,"props":1106,"children":1107},{},[1108],{"type":32,"value":1109},"Sezione espandibile:",{"type":32,"value":1111}," La sezione con i dettagli meteo aggiuntivi si espande e si riduce fluidamente grazie a GSAP.",{"type":26,"tag":108,"props":1113,"children":1114},{},[1115,1120],{"type":26,"tag":479,"props":1116,"children":1117},{},[1118],{"type":32,"value":1119},"Icone meteo dinamiche:",{"type":32,"value":1121}," Le icone meteo cambiano in modo reattivo in base alle condizioni meteo.",{"type":26,"tag":35,"props":1123,"children":1124},{},[1125],{"type":26,"tag":479,"props":1126,"children":1127},{},[1128],{"type":32,"value":1129},"Oltre a GSAP, ho curato l'interfaccia utente per renderla reattiva al tipo di meteo:",{"type":26,"tag":104,"props":1131,"children":1132},{},[1133,1143],{"type":26,"tag":108,"props":1134,"children":1135},{},[1136,1141],{"type":26,"tag":479,"props":1137,"children":1138},{},[1139],{"type":32,"value":1140},"Colori di sfondo:",{"type":32,"value":1142}," Lo sfondo dell'applicazione cambia dinamicamente in base alle condizioni meteo (es. azzurro per cielo sereno, grigio per nuvoloso).",{"type":26,"tag":108,"props":1144,"children":1145},{},[1146,1151],{"type":26,"tag":479,"props":1147,"children":1148},{},[1149],{"type":32,"value":1150},"Effetti visivi:",{"type":32,"value":1152}," Ho aggiunto effetti sottili come ombre e trasparenze per dare profondità all'interfaccia.",{"title":8,"searchDepth":192,"depth":192,"links":1154},[1155,1156],{"id":959,"depth":192,"text":962},{"id":1062,"depth":192,"text":1065},"content:works:weather-dashboard.md","works/weather-dashboard.md","works/weather-dashboard",1735324529522]